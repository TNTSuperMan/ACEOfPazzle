{
  "name": "runtime",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "buildbin": "electron-builder",
    "electron": "electron index.js"
  },
  "build": {
    "files": [
      "dist/html/*",
      "index.js",
      "package.json"
    ],
    "appId": "com.tntsuperman.acegame",
    "win": {
      "target": ["nsis", "zip"]
    },
    "mac": {
      "target": ["dmg", "zip"]
    },
    "linux": {
      "target": ["AppImage", "deb", "zip"]
    },
    "directories": {
      "output": "dist"
    }
  },
  "dependencies": {
    "bge-wasm": "^0.0.3",
    "libkey": "0.0.5"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/bun": "^1.2.4",
    "@types/license-checker": "^25.0.6",
    "@types/node": "^22.10.2",
    "electron": "^34.0.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.14.0",
    "license-checker": "^25.0.1",
    "npm-run-all2": "^7.0.2",
    "typescript": "~5.6.3",
    "vite": "^6.0.5",
    "vite-plugin-singlefile": "^2.1.0"
  }
}
